{
  "root": true,
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "plugins": ["import", "@typescript-eslint", "jest"],
  "extends": ["plugin:@typescript-eslint/recommended", "plugin:jest/recommended"],
  "ignorePatterns": ["webpack.config.js"]
}